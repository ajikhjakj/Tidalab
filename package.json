  "scripts": {
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win"
  },
  "build": {
    "electronVersion": "11.0.0",
    "productName": "测试app",
    "appId": "com.xiakja.tidalab",
    "copyright": "©2023 ",
    "directories": {
      "output": "build"
    },
    "files": [
      "main.js",
      "package.json",
      "**/*"
    ],
    "dmg": {
      "artifactName": "${productName}_setup_${version}.${ext}",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "artifactName": "${productName}_setup_${version}.${ext}",
      "icon": "./icons/icon.png",      
      "extraResources": [
        {
          "from": "./libs/darwin-x64",
          "to": "./libs/darwin-x64"
        },
        {
          "from": "./libs/geo",
          "to": "./libs/darwin-x64"
        },
        {
          "from": "./libs/config",
          "to": "./libs/darwin-x64"
        }
      ]
    },
    "win": {
      "artifactName": "${productName}_setup_${version}.${ext}",
      "icon": "./icons/icon.png",
      "extraResources": [
        {
          "from": "./libs/win32-x64",
          "to": "./libs/win32-x64"
        },
        {
          "from": "./libs/geo",
          "to": "./libs/win32-x64"
        },
        {
          "from": "./libs/config",
          "to": "./libs/win32-x64"
        }
      ],
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },  
