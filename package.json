{
  "name": "Tidalab",
  "main": "./src/main.js",
  "version": "4.1.10",
  "author": "Tidalab",
  "license": "CC0-1.0",
  "dependencies": {
    "express": "^4.17.1",
    "js-yaml": "^3.14.0",
    "kill-port": "^1.6.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "tcp-ping": "^0.1.1",
    "crypto-js": "^3.3.0"
